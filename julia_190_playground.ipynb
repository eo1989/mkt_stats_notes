    "using Printf\n",
    "\n",
    "function CelsiusToFahrenheit()\n",
    "    println(\"Celsius   Fahrenheit\\n\")\n",
    "    for c = 0:10:100  # start at 0, in steps of 10, up to 100 in \n",
    "        f = 32 + 9c / 5# 9c is valid && means 9*c\n",
    "        @printf(\"%5.0f %9.0f\\n\", c, f)\n",
    "    end\n",
    "end"
   ]
